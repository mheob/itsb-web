    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^5.0.1",
    "react-scrollspy": "^3.4.3",
    "react-transition-group": "^4.4.5",
    "react-typed": "^2.0.12",
    "react-waypoint": "^10.3.0"
  },
