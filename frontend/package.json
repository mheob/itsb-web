    "animate.css": "^4.1.0",
    "nuka-carousel": "^4.7.0",
    "react": "^16.13.1",
    "react-countup": "^4.3.3",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.10",
    "react-helmet-async": "^1.0.6",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-scripts": "^3.4.1",
    "react-scrollspy": "^3.4.2",
