    "@fortawesome/react-fontawesome": "^0.2.0",
    "animate.css": "^4.1.1",
    "nuka-carousel": "^8.0.0",
    "react": "^18.2.0",
    "react-countup": "^6.4.2",
    "react-dom": "^18.2.0",
