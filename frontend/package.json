    "@types/react-gtm-module": "^2.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-router-hash-link": "^1.2.1",
    "@types/react-scrollspy": "^3.3.2",
    "@types/react-transition-group": "^4.4.0",
    "eslint": "^7.4.0",
    "eslint-config-semistandard": "^15.0.1",
